/* pages/myaddress/index.wxss */
.address-list{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 100%;
}
.address{
  width: 710rpx;
  height: 200rpx;
  margin-top: 20rpx;
  border-radius: 10rpx;
  background: rgba(220,224,223,0.2);
  box-shadow: 1rpx 1rpx 2rpx 1rpx rgba(120,118,117,0.7);
  position: relative;
}
.address::after{
  content: "";
  position: absolute;
  top: 75rpx;
  right: 50rpx;
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-position: top center;
  background-size: cover;
  width: 50rpx;
  height: 50rpx;
}
.address view{
  margin-top: 20rpx;
}
.address-name{
  margin-left: 10rpx;
  width: 160rpx;
  font-size: 40rpx;
  float: left;
}
.address-phone{
  width: 500rpx;
  margin-left: 30rpx;
  float: left;
  font-size: 40rpx;
}
.address-address{
  margin-left: 10rpx;
  width: 500rpx;
  float: left;
  font-size: 40rpx;
}
.address-detail{
  margin-left: 10rpx;
  width: 100%;
  float: left;
  font-size: 40rpx;
}
.add-btn,.commit-btn{
  width: 710rpx;
  margin: 20rpx;
  text-align: center;
  height: 100rpx;
  line-height: 100rpx;
  border: 1rpx solid rgba(230,228,232,0.8);
  border-radius: 10rpx
}
.add-address{
  width: 100%;
  margin-top: 20rpx;
}
.add-address input{
  width: 710rpx;
  height: 100rpx;
  line-height: 100rpx;
  margin-left: 20rpx;
  border: 1rpx solid rgba(230,230,233,0.8);
  text-align: center;
  border-radius: 10rpx;
  margin-top: 20rpx;
}
.add-address-info{
  margin-left: 20rpx;
  color: rgba(120,118,122,0.7)
}
.add-address map{
  margin: 0 auto;
}