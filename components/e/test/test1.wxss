map {
  width: 100%;
  height: 500rpx;
}

.uplist-panel {
  background-color: #fff;
  border-bottom: 1rpx solid #eaeaea;
  display: flex;
  flex-direction: row;
  width: 100%;
  position: fixed;
  top: 0;
  height: 80rpx;
  z-index: 10;
}

.uplist-panel .uplist-item {
  width: 250rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 33rpx;
  color: #8a8a8a;
}

.uplist-panel .uplist-item.active {
  background:#f5f5f5;
}

.uplist-progress {
  position: absolute;
  top:80rpx;
  width: 250rpx;
  height: 5rpx;
  background: #ff7c63;
  z-index: 9;
  left:0;
}

.listbar {
  margin-top: 20rpx;
  width: 0.5%;
  height: 40rpx;
  background: #f0f0f0;
}


.Hover_button-panel{          /*悬浮按钮*/
  
  text-align: center;
  width:100rpx;
  height:100rpx;
  border-radius: 50%;
  background:#ce4c32;
  display: flex;
  align-items:center;

  opacity:0.9;
}
.Hover_button-panel image{
  margin:15rpx;
  width:70rpx;
  height:70rpx;
}



/*service页左侧列表栏*/

.goods_left {
  position: fixed;
  left: 0;
  display: inline-block;
  width: 25%;
  height: 100%;
  background: #f5f5f5;
  text-align: center;
  top: 80rpx;
}

.goods_left .goods_left_items {
  height: 80rpx;
  line-height: 80rpx;
  
  border-bottom: 1px dashed #dedede;
  font-size: 14px;
  color: #666;
}

.goods_left .goods_left_items.active {
  background: #fff;
  height: 120rpx;
  line-height: 120rpx;
}

.left_items_polish {
  position: absolute;
  left: 0%;
  height: 60rpx;
  width: 10rpx;
  background: #f8ca4f;
  top: 35rpx;
}

.goods_right {
  top:80rpx;
  position: absolute;
  right: 0;
  width: 75%;
  height: 100%;
  padding: 0rpx 20rpx 0rpx 20rpx;
  background: #fff;
  box-sizing: border-box;
}

.goods_right .goods_right_items {
  display: flex;
  flex-direction: row;
  width: 100%;
  padding-bottom: 20rpx;
  border-bottom: 1rpx solid #dadada;
  overflow: hidden;
}

.goods_right .goods_right_items.active {
  display: flex;
  flex-direction: column;
  width: 100%;
  border-bottom: 1rpx solid #dadada;
  overflow: hidden;
}

.goods_right .goods_right_items .top_panel {
  display: flex;
  flex-direction: row;
}

.goods_image {
  margin-top: 30rpx;
  width: 180rpx;
  height: 150rpx;
}

.righttext {
  position: relative;
  display: flex;
  flex-direction: column;
  margin-left: 50rpx;
  margin-top: 35rpx;
  height: 150rpx;
}

.righttext .name {
  width: 300rpx;
  font-size: 13px;
  word-wrap: break-word;
  word-break: break-all;
  text-overflow: ellipsis;
}

.righttext .price {
  position: absolute;
  bottom: 25rpx;
  width: 300rpx;
  font-size: 12px;
  color: red;
}

.addbutton {
  position: absolute;
  right: 50rpx;
  margin-top: 120rpx;
  width: 50rpx;
  height: 50rpx;
}

.goods_right .goods_right_items.active .bottom-panel {
  background: #fff;
  width: 100%;
  height: 60rpx;
  display: flex;
  flex-direction: row;
  
}
.goods_right .goods_right_items.active .bottom-panel text {
  margin:17rpx 0rpx 0rpx 50rpx;
  height:40rpx;
  font-size:30rpx;
  line-height:40rpx;
}
.goods_right .goods_right_items.active .bottom-panel input {
  margin:10rpx 0rpx 0rpx 10rpx;

  height:40rpx;
  border-bottom:1rpx solid #f8ca4f;
  font-size:30rpx;
  line-height:40rpx;
}



