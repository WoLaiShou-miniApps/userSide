}

.detail-image{
  margin-top:30rpx;
  min-height:200rpx;
  padding:20rpx 0rpx 20rpx 0rpx;
  border-top:1rpx solid #ededed;
  border-bottom:1rpx solid #ededed;
  background:#FFF;
  margin-bottom:120rpx;
  display: flex;
  flex-direction: column;
}
.detail-image .title{
  margin-left:20rpx;
  margin-bottom:50rpx;
  width:180rpx;
  height:30rpx;
  line-height:30rpx;
  padding-bottom:20rpx;
  border-bottom:1rpx solid #ededed;
  font-weight:500;
  text-align:center;
}
.detail-image image{
  width:80%;
  margin-left:10%;
  margin-bottom:20rpx;
}
.order-map{
  border-top:3rpx solid #ededed;
  border-bottom:3rpx solid #ededed;

  width: 750rpx;
  height: 400rpx;
  margin-top: 20rpx;
}


.rotate{
  animation:waiting 2s infinite;
  transform:rotate(0deg);
} 
@keyframes waiting{
  0% {
    transform:rotate(0deg);
  }
  50%{
    transform:rotate(90deg);
  }
  100%{
    transform:rotate(180deg);
  }
}
.shake{
  animation:shaking .5s infinite;
  transform:rotate(0deg) scaleX(-1);
}
@keyframes shaking{
  0% {
    transform:rotate(0deg) scaleX(-1);
  }
  33%{
    transform:rotate(5deg) scaleX(-1);
  }
  66%{
    transform:rotate(-5deg) scaleX(-1);
  }
  100%{
    transform:rotate(0deg) scaleX(-1);
  }
}

