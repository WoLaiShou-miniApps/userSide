}

.time_stamp{
  font-size:30rpx;
  width:auto;
  height:50rpx;
  line-height:50rpx;

  text-align:right;
  margin-right:30rpx;
}

.user_title{
  margin:30rpx 0 30rpx 50rpx;
  font-size:33rpx;
  font-weight:bold;
  font-family:'Courier New', Courier, monospace;
}

.driver_title{
  margin:70rpx 0 30rpx 50rpx;
  font-size:33rpx;
  font-weight:bold;
  font-family:'Courier New', Courier, monospace;
}


.detail-chief{
  width:100%;
  border-bottom:1rpx solid #ededed;
  padding-top:20rpx;
  padding-bottom:20rpx;
  background: #FFF;
}
.detail-chief .title{
  margin:30rpx 0rpx 100rpx 35rpx;

  font-size:55rpx;
  font-weight:700;
}

.detail-chief .term{
  margin-top:20rpx;
}
.detail-chief .term .term-item{
  width:90%;
  min-height:100rpx;
  border-bottom:1rpx solid #efefef;
  margin:0% 5% 0% 5%;
  display: flex;
  flex-direction: row;
  line-height:100rpx;
}
.detail-chief .term .term-item .image{
  width:50rpx;
  height:50rpx;
  margin-top:25rpx;
  margin-left:20rpx;
}
.detail-chief .term .term-item .tap_image{
  width:50rpx;
  height:50rpx;
  margin-top:25rpx;
  position:absolute;
  right:50rpx;
  animation:LR_mobiling 1s infinite;
}

@keyframes LR_mobiling{
  0%{
    transform:scale(-1,1);
  }
  60%{
    transform:scale(-1,1) translateX(40rpx);
  }
  100%{
    transform:scale(-1,1);
  }
}

.detail-chief .term .term-item .text{
  word-wrap : break-word;
  margin-top:35rpx;
  margin-bottom:35rpx;
  width:500rpx;
  min-height:30rpx;
  display:block;
  font-size:30rpx;
  color:#616161;
  font-weight:500;
  margin-left:50rpx;
  line-height:35rpx;
}
.more-right{
  margin-top:25rpx;
  position:absolute;
  right:6%;
  width:25rpx;
  height:50rpx
}

.detail-remark{
  margin-top:30rpx;
  padding:20rpx 0rpx 20rpx 0rpx;
  min-height:200rpx;
  width:100%;
  background:#FFF;
  border-top:1rpx solid #ededed;
  border-bottom:1rpx solid #ededed;

  display: flex;
  flex-direction: column;

}
.detail-remark .title{
  margin-left:20rpx;
  margin-bottom:20rpx;
  width:180rpx;
  height:30rpx;
  line-height:30rpx;
  padding-bottom:20rpx;
  border-bottom:1rpx solid #ededed;
  font-weight:500;
  text-align:center;
}

.detail-voice{
  margin-top:30rpx;
  margin-bottom:50rpx;
  padding:20rpx 0rpx 20rpx 0rpx;
  min-height:200rpx;
  width:100%;
  background:#FFF;
  border-top:1rpx solid #ededed;
  border-bottom:1rpx solid #ededed;
}
.detail-voice .title{
  margin-left:20rpx;
  margin-bottom:20rpx;
  width:180rpx;
  height:30rpx;
  line-height:30rpx;
  padding-bottom:20rpx;
  border-bottom:1rpx solid #ededed;
}
.detail-voice image{
  margin-left:300rpx;
  vertical-align: middle;
  width:100rpx;
  height:100rpx;
}

.detail-remark text{
  margin-left:50rpx;
  
  word-wrap: break-word;
  word-break:break-all;
