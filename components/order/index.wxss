page{
  background:#f5f5f9;
}
.uplist-panel{
  position:fixed;
  top:-3rpx;
  width:100%;
  height:100rpx;
  background:#FFF;
  display: flex;
  flex-direction: row;
  z-index: 99;
}
.uplist-panel .item{
  width:50%;
  height:100rpx;
  font-size:35rpx;
  line-height:100rpx;
  text-align:center;
}
.uplist-panel .item.active{
  color:rgb(199,22,34);
  border-bottom:5rpx solid rgb(199,22,34);
}

.empty-list{
  margin-top:150rpx;
  width:100%;
  text-align:center;
  font-size:30rpx;
}
.myorder_list{
  margin-top:70rpx;
  padding-top:30rpx;
  padding-bottom:30rpx;
  border:1rpx double #ededed;
}
.more-down{
  margin-top:40rpx;
  position:absolute;
  right:6%;
  width:50rpx;
  height:25rpx
}
.operate-panel{
  margin-left:5%;
  width:90%;
  height:100rpx;

  padding:25rpx 0rpx 0rpx 0rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-end;

  border-top:1rpx dashed #d4d4d4;
}
.operate-panel .item{
  margin-right:10rpx;
  margin-left:40rpx;
  padding:5rpx 15rpx 5rpx 15rpx;
  height:70rpx;
  line-height:70rpx;
  font-size:32rpx;
  color:#FFF;
  border-radius: 10rpx;
  background:#1AC7B8;
}
.operate-panel .item:active{
  background:#21fbe8;
  margin-right:5rpx;
  margin-top:-5rpx;
  padding:10rpx 25rpx 15rpx 25rpx;
}

.orders-item_outside{
  background:#FFF;
  padding-top:20rpx;
  border-top:1rpx solid #d4d4d4;
  border-bottom:1rpx solid #d4d4d4;
  position: relative;
}
.orders-item_outside.selected{
  margin-top:20rpx;
  margin-bottom:20rpx;
}

.orders-item{
  margin-left:5%;

  height:200rpx;
  width:90%;

  display: flex;
  flex-direction: row;
}
.orders-item.invalid{
  color:#C4C4C4 !important;
}
.orders-item .name{
  margin-left:20rpx;
  height:100rpx;
  width:200rpx;
  line-height:100rpx;
  font-size:35rpx;
  white-space:nowrap;
  text-overflow:ellipsis;
  overflow:hidden;
}

.orders-item .righttext{
  display: flex;
  flex-direction: column;

  width:300rpx;

  font-size:30rpx;
  line-height:50rpx;
}
.orders-item .righttext .address{
  width:300rpx;
  white-space:nowrap;
  text-overflow:ellipsis;
  overflow:hidden;
}
.orders-item .righttext .time{
  width:300rpx;
  white-space:nowrap;
  text-overflow:ellipsis;
  overflow:hidden;
}

.del-node{
  margin-top:15rpx;
  position:absolute;
  right:18%;
  width:70rpx;
  height:70rpx;
  z-index:initial;
}

.timeStamp_module{
  font-size:30rpx;
  position:absolute;
  top:150rpx;
  right:20rpx;
  width:350rpx;
  height:60rpx;
  text-align:right;
  line-height:50rpx;
  color:#b5b5b5;
}


.contact-module{
  position:absolute;
  top:145rpx;

  width:360rpx;
  height:60rpx;
  background:#C4C4C4;
  border-top-right-radius:50rpx;
  border-bottom-right-radius:50rpx;
  display: block;
  line-height:50rpx;
  animation:right-mobiling 1s;
}
.contact-module:active{
  background:#e4e3e3;
}

@keyframes right-mobiling{
  0%{
    transform: translateX(-300rpx)
  }
  100%{
    transform: translateX(0rpx)
  }
}


.contact-module image{
  display: inline-block;
  vertical-align: middle;
  width:50rpx;
  height:50rpx;
  margin-left:30rpx;
  transform: scale(-1,1);
}
.contact-module .text{
  display: inline-block;
  margin-left:20rpx;
  color:#FFF;
  font-size:30rpx;
}
/*orderdetail页*/
