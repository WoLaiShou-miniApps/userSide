.banjia {
  color: #fff;
  top: 285rpx;
  position: relative;
  width: 300rpx;
  margin-left: 225rpx;
  text-align: center;
  height: 300rpx;
  line-height: 300rpx;
  border: 1rpx solid #C51A29;
  border-radius: 50%;
  background: #C51A29;
}

.banjia image {
  margin-top: 65rpx;
  width: 50%;
  height: 50%;
}

.banjia-info {
  color: #fff;
  position: relative;
  margin-top: 215rpx;
  text-align: center;
  font-size: 14px;
}

.banjia-text {
  color: #373e40;
  position: relative;
  margin-top: 175rpx;
  text-align: center;
  font-size: 14px;
  padding: 0 100rpx;
  opacity: 0.7;
}

.banjia-text2 {
  color: #373e40;
  position: relative;
  margin-top: 20rpx;
  text-align: center;
  font-size: 14px;
  padding: 0 100rpx;
  opacity: 0.7;
}

.banjia_order {
  margin-top: 200rpx;
}

.input-area {
  width: 710rpx;
  margin-left: 20rpx;
}

.input-area input {
  text-align: center;
  height: 100rpx;
  line-height: 100rpx;
  border-radius: 10rpx;
  box-shadow: 1rpx 1rpx 1rpx 1rpx rgba(35, 34, 33, 0.2) inset;
  border:solid 1rpx #000;
}

.btn-area {
  width: 710rpx;
  margin-left: 20rpx;
  margin-top: 30rpx;
}
.btn-area button{
  color: #fff;
  background: #C51A29;
  border: 1rpx solid #C51A29;
  height: 100rpx;
  line-height: 100rpx;
}
.banjia_order_info{
  margin-top: 100rpx;
  width: 710rpx;
  margin-left:20rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  box-shadow: 1rpx 1rpx 1rpx rgba(30,33,39,0.4);
  border: 1rpx solid rgba(30,33,39,0.4);
}
.banjia_order_info text{
  width: 100%;
  display: block;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 35rpx;
  color: 373e40;
  opacity: 0.9;
  text-align: center;
}
.moveOrderList{
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.moveOrder{
  width: 700rpx;
  background: #f5f6eb;
  height: 400rpx;
  margin-top: 20rpx;
  box-shadow: 1rpx 1rpx 1rpx 1rpx rgba(99,99,99,0.3);
  border-radius: 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.order-info{
  margin-left: 20rpx;
  color: #34352c;
}
.call-for-service{
  width: 700rpx;
  margin:25rpx;
  text-align: center;
  color: #f5f6eb;
  text-shadow: 1rpx 1rpx 1rpx rgba(30,30,30,0.9);
  height: 100rpx;
  line-height: 100rpx;
  box-shadow: 1rpx 1rpx 1rpx 1rpx rgba(220,220,220,0.8);
 background-image: url("data:image/png;base64,");
 background-repeat: no-repeat;
 background-position: center;
}
.btn{
  width:500rpx;
  height:100rpx;
  text-align:center;
  line-height:100rpx;
  border:1rpx solid rgba(30,30,30,0.3);
  border-radius:10rpx;
  margin-left:85rpx;
}
.done{
  background:#b3b3b3;
  position: relative;
}
.done .order-info{
  color: #d3d3d3;
}
.done .btn{
  border:1rpx solid #d3d3d3;
  color: #d3d3d3;
}
.done::after{
  content:"订单终结";
  position:absolute;
  width:200rpx;
  height:100rpx;
  border:3rpx solid #000;
  top:150rpx;
  left:250rpx;
  transform:rotate(-35deg);
  color:#000;
  text-align:center;
  line-height:80rpx;
  font-weight:900;

}